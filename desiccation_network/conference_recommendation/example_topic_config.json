{
    "sent_trans_model": "allenai/scibert_scivocab_cased",
    "umap_params": {
        "n_neighbors": 15,
        "n_components": 5,
        "min_dist": 0,
        "metric": "cosine",
        "random_state": 42
    },
    "vectorizer_params": {
        "stop_words": "english",
        "ngram_range": [
            1,
            3
        ],
        "min_df": 10
    },
    "representation_model_name": "openai",
    "representation_params": {
        "model": "gpt-3.5-turbo",
        "chat": true
    },
    "openai_api_key_path": "../../data/openai_key.py",
    "outlier_reduction": true,
    "outlier_reduction_params": {
        "strategy": "embeddings",
        "threshold": 0.1
    }
}
